                                ]
                            },
                            "SpritePath": {
                                "type": "string",
                                "default": "",
                                "description": "设置背景图片的路径",
                                "title": "The SpritePath Schema",
                                "examples": [
                                    "UI/Textures/Icon/Interlude/YYSW/yysw_liushengji"
                                ]
                            }
                        }
                    }
                },
                {
                    "if": {
                        "properties": { "__type": { "const": "InterludeEvent.ShowCenterDialogOption,Assembly-CSharp" } }
                    },
                    "then": {
                        "properties": {
                            "Localization_Dialog": {
                                "type": "object",
                                "description": "显示讲述文本",
                                "additionalProperties": true,
