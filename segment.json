                {
                    "if": {
                        "properties": { "__type": { "const": "InterludeEvent.SetInterludeTxtBg,Assembly-CSharp" } }
                    },
                    "then":
                    {
                        "properties": {
                            "SetMode": {
                                "type": "integer",
                                "default": 0,
                                "description": "设置为显示或是消失",
                                "title": "The SetMode Schema",
                                "examples": [
